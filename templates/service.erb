
# <%= @name %>
define service {
  check_command <%= @check_command_str %>
<% if @host_list %>  host_name <%= @host_list %><% end %>
<% if @hostgroup_list %>  hostgroup_name <%= @hostgroup_list %><% end %>
<% if @description %>  service_description <%= @description %><% end %>
<% if @use %>  use <%= @use %><% end %>
<% if @contact_groups_list %>  contact_groups <%= @contact_groups_list %><% end %>
<% if @notes %>  notes <%= @notes %><% end %>
}
